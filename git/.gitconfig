[format]
	signOff = true
[core]
	autocrlf = input
	ignorecase = false
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
        #conflictstyle = diff3
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[commit]
	gpgsign = true
[pull]
	ff = only
	rebase = true
